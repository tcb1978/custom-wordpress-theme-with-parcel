{
  "name": "custom-theme-with-parcel",
  "version": "1.0.0",
  "main": "/src/js/index.js",
  "repository": "https://github.com/tcb1978/custom-wordpress-theme-with-parcel.git",
  "author": "Matthew Eldredge credit to: Jonathan Christopher for the bundler",
  "license": "MIT",
  "scripts": {
    "watch": "concurrently \"yarn run dev\" \"yarn run build\"",
    "dev": "cross-env NODE_ENV=development node src/js/bundler-dev.js",
    "build": "cross-env NODE_ENV=production node src/js/bundler-build.js",
    "start": "parcel parcel-bundler -p 3001"
  },

  "dependencies": {
    "jquery": "^3.3.1",
    "autoprefixer": "^8.6.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.8.1",
    "postcss-modules": "^1.1.0",
    "scss-loader": "0.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tcb1978/custom-wordpress-theme-with-parcel.git"
  }
}
